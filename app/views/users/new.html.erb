<%= form_with model: @user, url: "/users/create", local: true do |form| %>
    <% @user.errors.full_messages.each do |message| %>
        <p><%= message %></p>
    <% end %>
    <%= form.text_field :name %>
    <%= form.email_field :email %>
    <%= form.password_field :password %>
    <%= form.submit %>
<% end %>
